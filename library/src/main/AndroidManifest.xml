<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.buffalo.cse.android.maybelibrary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <service
            android:name="edu.buffalo.cse.android.maybelibrary.QueryIntentService"
            android:exported="false">
        </service>

        <receiver android:name="edu.buffalo.cse.android.maybelibrary.PullReceiver">
            <intent-filter android:priority="1">
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="edu.buffalo.cse.android.maybelibrary.pull_intent" />
            </intent-filter>
        </receiver>
        <!--[START log_intentserver] -->
        <service
            android:name="edu.buffalo.cse.android.maybelibrary.LogIntentService"
            android:permission="com.google.android.gms.permission.BIND_NETWORK_TASK_SERVICE"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.android.gms.gcm.ACTION_TASK_READY"/>
            </intent-filter>
        </service>
        <!--[END log_intentserver] -->

        <!-- [START gcm_receiver] -->
        <receiver
            android:name="com.google.android.gms.gcm.GcmReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <category android:name="edu.buffalo.cse.android.maybelibrary" />
            </intent-filter>
        </receiver>
        <!-- [END gcm_receiver] -->

        <service
            android:name="edu.buffalo.cse.android.maybelibrary.MyGcmListenerService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            </intent-filter>
        </service>
        <service
            android:name="edu.buffalo.cse.android.maybelibrary.MyInstanceIDListenerService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.android.gms.iid.InstanceID" />
            </intent-filter>
        </service>
    </application>

</manifest>
